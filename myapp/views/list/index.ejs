<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>list</title>
    <style>
      .container{
        width:450px;
        height:300px;
        background-color: aqua;
        text-align:center;
        margin: 10px;
      }
    </style>
  </head>
  <body>
    <div class="container">
          <div class="kontak"><br>
      <% list.forEach((el, i) => { %>
        <h3>Nama Barang</h3><b>
        <p><%= el.nama_barang %></p>
      </div>
      <div class="kontak">
        <h3>Jumlah Barang</h3><b>
          <p><%= el.jumlah_barang %></p>
          </div>
          <div class="kontak">
        <h3>Harga Barang</h3><b>
        <p><%= el.harga_barang %></p>
        <a href="/list/<%= el.id %>">Delete</a>
        <!-- <form class="button-submit" action="/list" method="post">
          <button type="submit" name="button">DELETE</button>
        </form> -->
</div>
</div>

      <% }); %>
  </body>
</html>
